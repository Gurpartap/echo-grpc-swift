{
  "name": "EchoSDK",
  "version": "0.0.1",
  "license": "MIT",
  "platforms": {
    "ios": "7.1",
    "osx": "10.9"
  },
  "prepare_command": "mkdir -p ../EchoClient/Pods/EchoSDK\nprotoc -I . --objc_out=../EchoClient/Pods/EchoSDK --objcgrpc_out=../EchoClient/Pods/EchoSDK ./*.proto",
  "subspecs": [
    {
      "name": "Messages",
      "source_files": [
        "../EchoClient/Pods/EchoSDK/*.pbobjc.{h,m}",
        "../EchoClient/Pods/EchoSDK/**/*.pbobjc.{h,m}"
      ],
      "header_mappings_dir": "../EchoClient/Pods/EchoSDK",
      "requires_arc": false,
      "dependencies": {
        "Protobuf": [

        ]
      }
    },
    {
      "name": "Services",
      "source_files": [
        "../EchoClient/Pods/EchoSDK/*.pbrpc.{h,m}",
        "../EchoClient/Pods/EchoSDK/**/*.pbrpc.{h,m}"
      ],
      "header_mappings_dir": "../EchoClient/Pods/EchoSDK",
      "requires_arc": true,
      "dependencies": {
        "gRPC": [

        ],
        "EchoSDK/Messages": [

        ]
      }
    }
  ]
}
